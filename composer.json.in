  "authors": [
    {
      "name": "Claus-Justus Heine"
    }
  ],
  "require": {
    "behat/transliterator": "^1.3.0",
    "chillerlan/php-qrcode": "^4.2",
    "cmixin/business-day": "^1.12",
    "components/jquery": "^2.2.0",
    "components/jquery-migrate": "^3.0.0",
    "composer/spdx-licenses": "^1.5",
    "dompdf/dompdf": "^1.0",
    "ezyang/htmlpurifier": "^4.13.0",
    "giggsey/libphonenumber-for-php": "^8.12",
    "globalcitizen/php-iban": "^4.0",
    "guzzlehttp/guzzle": "^7.0",
    "html2text/html2text": "^4.3",
    "jpgraph/jpgraph": "*",
    "kwn/number-to-words": "^2.8",
    "league/html-to-markdown": "^5.1",
    "maennchen/zipstream-php": "^3.0.0",
    "mpdf/mpdf": "^8.0",
    "paragonie/halite": "^5",
    "pear/auth_sasl": "dev-master",
    "pear/mail": "dev-master",
    "pear/net_imap": "dev-master",
    "phpmailer/phpmailer": "^6.3.0",
    "phpoffice/phpspreadsheet": "^1.21.0",
    "rotdrop/bav": "dev-master",
    "setasign/fpdf": "*",
    "spatie/temporary-directory": "^1.3",
    "symfony/var-dumper": "^5.2",
    "tecnickcom/tcpdf": "^6.3",
    "tinybutstrong/opentbs": "dev-production/cafevdb",
    "tuqqu/gender-detector": "^0.5.2"
  },
  "require-dev": {
    "bamarni/composer-bin-plugin": "^1.8",
    "nextcloud/coding-standard": "*",
    "phpmd/phpmd": "@stable",
    "phpunit/phpunit": "*",
    "squizlabs/php_codesniffer": "^3.6.0",
    "zebra-north/phpcs-short-types": "^1.0"
  },
  "autoload": {
    "classmap": ["3rdparty/phpMyEdit/."]
  },
  "repositories": [
    {
      "type": "path",
      "url": "3rdparty/bav"
    },
    {
      "type": "path",
      "url": "3rdparty/php-index"
    },
    {
      "type": "path",
      "url": "3rdparty/opentbs"
    },
    {
      "type": "path",
      "url": "3rdparty/pear/mail"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "bamarni-bin": {
      "bin-links": false,
      "target-directory": "vendor-bin",
      "forward-command": true
    }
  }
}

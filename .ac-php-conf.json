{
  "use-cscope": null,
  "tag-dir": null,
  "filter": {
    "php-file-ext-list": [
      "php"
    ],
    "php-path-list": [
      ".",
      "../../3rdparty",
      "../../lib",
      "../dokuwikiembedded/lib",
      "../redaxo4embedded/lib",
      "../files_trashbin/lib"
    ],
    "php-path-list-without-subdir": [],
    "ignore-ruleset": [
      "# like .gitignore file ",
      "/vendor*/**/polyfill-php80/**/*.php",
      "/vendor*/**/polyfill-php81/**/*.php",
      "/vendor*/humbug/php-scoper/**",
      "/build/**"
    ]
  }
}

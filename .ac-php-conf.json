{
  "use-cscope": null,
  "tag-dir": null,
  "filter": {
    "can-use-external-dir": true,
    "php-file-ext-list": [
      "php"
    ],
    "php-path-list": [
      ".",
      "../../3rdparty",
      "../../lib",
      "../dav/lib",
      "../dokuwiki/lib",
      "../redaxo4embedded/lib",
      "../files_trashbin/lib",
      "../files/lib",
      "../cafevdbmembers/lib",
      "../user_sql/lib"
    ],
    "php-path-list-without-subdir": [],
    "ignore-ruleset": [
      "# like .gitignore file ",
      "vendor*/**/polyfill-php80/**/*.php",
      "vendor*/**/polyfill-php81/**/*.php",
      "vendor*/humbug/php-scoper/**",
      "build/**",
      "3rdparty/**/[tT]ests/**/*.php",
      "vendor/**/[tT]ests/**/*.php",
      "3rdparty/**/[Ee]xamples/**/*.php",
      "vendor/**/[Ee]xamples/**/*.php",
      "vendor/composer/*.php",
      "vendor/*.php",
      "vendor/squizlabs/php_codesniffer/**/*.php",
      "node_modules/**"
    ]
  }
}

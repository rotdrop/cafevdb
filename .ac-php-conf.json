{
  "use-cscope": null,
  "tag-dir": null,
  "filter": {
    "php-file-ext-list": [
      "php"
    ],
    "php-path-list": [
      ".",
      "../../3rdparty",
      "../../lib"
    ],
    "php-path-list-without-subdir": [],
    "ignore-ruleset": [
      "# like .gitignore file ",
      "/vendor*/**/polyfill-php80/**/*.php",
      "/vendor*/humbug/php-scoper/**",
      "/build/**"
    ]
  }
}
